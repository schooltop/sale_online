.dropmenu {
	border:solid 1px #638bc8;
	border-bottom:0;
	width:80px;
	filter:alpha(opacity=65);
	color:#1670c8;
	text-align:left;
}

.dropmenu a:hover {
	background:url(images/dropmenu_selected.gif) left center no-repeat;
}

.dropmenu a{
	background:url(images/dropmenu_selected.gif) left center no-repeat #fff;
	padding:6px 8px 3px 20px;
	text-decoration: none;
	color:#000;
	font-size:12px;
	display:block;
	border-bottom:solid 1px #638bc8;
}

.dropwin {
	width:340px;
	filter:alpha(opacity=100);
	background-color: #ced7e8;
	background-position: left bottom;
	background-image: url(images/023.gif);
}


.dropwin td {
	font-size:12px;
	color:#1670c8;
}

.dropwin_border {
	padding:7px 2px 2px 2px;
}

.dropwin_button{
	padding:13px;
	padding-bottom:5px;
	border-bottom:solid 1px #6dbedf;
}

.dropwintab li{
	background:url(images/dropwin_button_bg.gif) #4759a6 left top repeat-x;
	padding:5px 12px;
	color:#fff;
	display:block;
	font-size:12px;
	list-style:none;
	border:solid 1px #0c1556;
	float:left;
	border-bottom:0;
	margin-left:3px;
	margin-right:3px;
	cursor:hand;
}

.dropwintab .hottab{
	background:url(images/dropwin_button_hot_bg.gif) #5b6998 left top repeat-x;
	color:#fff;
	display:block;
	font-size:12px;
	list-style:none;
	float:left;
	border:solid 1px #0c1556;
	border-bottom:0;
	font-weight: bold;
	padding: 5px;
}

.dropwin_form {
	padding:13px;
	border-top:0;
	margin:0;
	text-align:center
}

.dropwin_input {
	width:212px;
	height:22px;
	border:solid 1px #454d7b;
}

.dropwin_submit {
	height:23px;
	background:url(images/dropwin_submit_bg.gif) #181c1d left top repeat-x;
	border:solid 1px #454d7b;
	font-size:12px;
	color:#fff;
	padding-top:2px;
}

.dropwin .tab {
	float:left;
}

.dropwin .close {
	float:right;
}
